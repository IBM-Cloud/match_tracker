    require (
    {baseUrl: "http://static.bbci.co.uk/sport/2.7.640/desktop/modules/"},
    ["jquery-1", "jsonloader", "livescores", "livescoresui"],
    function($, jsonloader, liveScores, liveScoresUI) {
        $(document).ready(function() {
            window.jsonLoader.model.addModule({
                renderer: {
                    src: liveScores,
                    id: "livescores"
                },
                moduleObject: {
                    id: "livescores"
                },
                feedObject: {
                    updatePeriod: 30,
                    dataSource:"http://polling.bbc.co.uk/sport/shared/football/live-scores/competition/118996114/20160321.json",
                    getDataOnLoad:false
                }
            });

            liveScoresUI.init("http://polling.bbc.co.uk", "Mon, 21 Mar 2016 00:00:00 +0000", "118996114");
        });
        // Force IE7 to show the controls.
        $("td.match-score").load(function() {
            $(".live-scores-day-navigator").show();
            $(".live-scores-dropdowns").show();
            $(".refresh-icon").show();
            $("#refresh-text").show();
            $("td.match-score").addClass('hover-enabled').addClass('live-scores-show');
        });
    }
);
